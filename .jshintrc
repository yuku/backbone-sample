{
  "strict"   : true,
  "unused"   : true,
  "noarg"    : true,

  "expr"     : true,
  "eqnull"   : true,

  // keywords
  "undef"    : true,
  "browser"  : true,
  "jquery"   : true,
  "globals"  : {
    "_"              : false,
    "Backbone"       : false,

    // RequireJS
    "define"         : false,
    "require"        : false,

    // Jasmine
    "jasmine"        : false,
    "describe"       : false,
    "xdescribe"      : false,
    "it"             : false,
    "xit"            : false,
    "expect"         : false,
    "beforeEach"     : false,
    "afterEach"      : false,
    "spyOn"          : false,

    // Qunit
    "module"         : false,
    "test"           : false,
    "asyncTest"      : false,
    "start"          : false,
    "stop"           : false,
    "deepEqual"      : false,
    "equal"          : false,
    "notDeepEqual"   : false,
    "notEqual"       : false,
    "notStrictEqual" : false,
    "ok"             : false,
    "strictEqual"    : false,
    "throws"         : false
  }
}

